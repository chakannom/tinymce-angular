{
  "name": "@tinymce/tinymce-angular",
  "version": "2.5.0",
  "description": "Official TinyMCE Angular Component",
  "author": "Ephox Inc",
  "license": "Apache-2.0",
  "scripts": {
    "clean": "rimraf lib",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "build:package": "npm run clean && ng-packagr -p tinymce-angular-component/ng-package.json && copyfiles README.md lib/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prepare": "npm run build:package",
    "publish": "npm run build:package && (cd lib && npm publish)",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/core": "^7.2.2",
    "@angular/animations": "^7.2.1",
    "@angular/common": "^7.2.1",
